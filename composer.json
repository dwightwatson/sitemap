{
    "name": "watson/sitemap",
    "description": "Generate Google Sitemaps in Laravel",
    "keywords": [
        "laravel",
        "sitemaps"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@studious.com.au"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src/Watson"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Sitemap\\SitemapServiceProvider"
            ],
            "aliases": {
                "Sitemap": "Watson\\Sitemap\\Facades\\Sitemap"
            }
        }
    }
}
